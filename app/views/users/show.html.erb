<% @posts.each do |post| %> 
    <h2> @<%= post.user.nickname %> </h2>
    <h5> <%= post.title %> </h5>
    <h6> <%= post.description %> </h6>    
    <p> <small class="text-muted"> <%= post.created_at %></small></p>
   <% if post.images.attached? %> 
  <% post.images.each do |image| %>
  <div>	<%= image_tag image, style: 'width: 340px' %> </div>
  <% end %> 
<% end %> 
<% end %> 